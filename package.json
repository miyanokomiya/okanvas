{
  "name": "okanvas",
  "version": "0.0.4",
  "main": "./dist/okanvas.min.js",
  "module": "./dist/okanvas.esm.min.js",
  "types": "./dist/okanvas.d.ts",
  "sideEffects": false,
  "repository": "https://github.com/miyanokomiya/okanvas.git",
  "author": "miyanokomiya <tomoya.hstexze24632@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "demo": "parcel demo/index.html",
    "lint:es": "yarn eslint ./src ./test ./demo ./*.js --ext .js,.ts",
    "lint:json": "yarn prettier -c ./*.json ./.*.json",
    "lint": "yarn lint:es && yarn lint:json",
    "lint:fix": "yarn lint:es --fix && yarn lint:json --write",
    "test": "jest --coverage",
    "build": "rm -rf ./dist && yarn bili --format cjs-min --format esm-min",
    "build:demo": "rm -rf ./dist-demo && parcel build demo/index.html -d ./dist-demo --public-url .",
    "prepublishOnly": "yarn build"
  },
  "devDependencies": {
    "@types/jest": "^26.0.9",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.23.0",
    "babel-jest": "^26.2.2",
    "bili": "^5.0.5",
    "canvas": "^2.6.1",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.23.1",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "jest": "^26.2.2",
    "jsdom": "^16.4.0",
    "parcel-bundler": "^1.11.0",
    "preact": "^10.5.7",
    "prettier": "^2.3.0",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-jest": "^26.1.4",
    "typescript": "^4.2.4"
  },
  "dependencies": {}
}
